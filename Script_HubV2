local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
	Title = "scripts_hub",
	Style = 4,
	SizeX = 400,
	SizeY = 400,
	Theme = "Light",
	ColorOverrides = {
		MainFrame = Color3.fromRGB(235,235,235)
	}
})

local Y = X.New({
	Title = "UI's"
})

local Z = X.New({
	Title = "Hub"
})

local W = X.New({
	Title = "hub (keys)"
})

local C = X.New({
	Title = "discord"
})

local A = Y.Button({
	Text = "Epic",
	Callback = function()
		loadstring(game:HttpGet(""))();
	end,
	Menu = {
		Information = function(self)
			X.Banner({
				Text = "One Punch Destiny"
			})
		end
	}
})

